{
    "name": "Pori Events",
    "description": "Drupal 8 based Pori Events Composer Project",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "slick",
                "version": "1.6.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/1.6.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/config_installer": "~1.0",
        "drupal/warden": "^1.1",
        "drupal/simplei": "^1.1",
        "wunderio/drupal-ping": "^1.0",
        "drupal/address": "^1.3",
        "drupal/elasticsearch_helper": "5.x-dev",
        "drupal/admin_toolbar": "^1.23",
        "drupal/coffee": "^1.0",
        "ccampbell/chromephp": "^4.1",
        "drupal/migrate_plus": "^4.0",
        "drupal/migrate_tools": "^4.0",
        "drupal/taxonomy_multidelete_terms": "^1.1",
        "drupal/structured_data": "^1.0",
        "drupal/imagecache_external": "^1.0",
        "drupal/entityqueue": "^1.0",
        "drupal/slick": "^1.0",
        "drupal/slick_views": "^1.0",
        "drupal/fontawesome": "^2.2",
        "drupal/twig_tweak": "^2.0",
        "drupal/google_analytics": "^2.3",
        "slick": "^1.6",
        "blazy": "^1.8",
        "drupal/simple_gmap": "^1.4",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/date_range_formatter": "^3.1"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "codeception/visualception": "^0.9.0",
        "guncha25/codecept-drupal-drush": "~0.3",
        "guncha25/drupal-8-bootstrap": "0.3",
        "drupal/config_devel": "^1.2",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/devel": "^2.0",
        "drupal/twig_xdebug": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "dropin-paths": {
            "web/": ["type:web-dropin"]
        },
        "patches": {
            "drupal/core": {
                "Migrate delete functionality": "https://www.drupal.org/files/issues/2019-01-30/2809433-migrate-delete-missing-from-source-81.patch"
            },
            "drupal/migrate_tools": {
                "Migrate delete command": "https://www.drupal.org/files/issues/2019-01-22/migrate-tools-rollback-missing-items-from-source-2863426-20.patch",
                "Fix for command parameters": "https://www.drupal.org/files/issues/2019-02-05/migrate-tools_fix-drush-options_3024399-23.patch"
            }
        }
    }
}
